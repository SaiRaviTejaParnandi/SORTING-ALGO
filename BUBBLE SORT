#include <iostream>

using namespace std;

int main()
{
    int a[5]={5,8,1,6,3};
    int i,j=0;
    for (i=0;i<4;i++)
    {
        for(j=i+1;j<=4;j++)
        {
            if(a[i]>a[j])
            {
                swap(a[i],a[j]);
            }
        }
    }
  for (int i=0;i<=4;i++)
  {
      cout<<a[i]<<" ";
  }
    return 0;
}
